pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                echo 'Checkout'
            }
        }

        stage('Build') {
            steps {
                echo 'your_build_command_here'
            }
        }

        stage('Deploy') {
            steps {
                echo 'your_deploy_command_here'
            }
        }
    }
}
