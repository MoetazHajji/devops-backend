pipeline {
    agent any
    
    stages {

        stage('Login') {
            steps {
                script {
                    sh 'echo "123456789" |  docker login --username iheblafi  --password-stdin'
                }
            }
        }
       

      

        stage('Docker-compose') {
           steps {
              script {
                sh 'docker compose up -d'
              }
                
            }
        }

    }
      }
