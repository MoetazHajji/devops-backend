pipeline {
    agent any
    stages {
        stage('Stage Maven') {
            steps {
                // Cette Ã©tape va nettoyer et compiler le projet avec Maven
                sh 'mvn clean compile install -DskipTests'
            }
        }
    }

    post {
             success {
                 echo 'Successfully.'
             }
             failure {
                 echo 'Failed'
             }
         }

}